    <div class="container-fluid w-75 pt-5">
      <div class="pb-4 d-flex justify-content-between">
        <h1>Listado de Usuarios</h1>
        <a href="/usuarios/crear" type="submit" class="btn btn-success mb-3">Nuevo Usuario</a>
      </div>
      <table class="table table-light table-bordered table-striped">
        <thead>
          <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>
            <th scope="col">Apellido</th>
            <th scope="col">Edad</th>
            <th scope="col">Acciones</th>
          </tr>
        </thead>
        <tbody>
          <% for (user of users) {%>
          <tr>
            <td><%=user.id%></td>
            <td><%=user.firstname%></td>
            <td><%=user.lastname%></td>
            <td><%=user.age%></td>
            <td>
              <a href="/usuarios/editar/<%=user.id%>" type="submit" class="btn btn-primary mb-3"
                >Editar</a
              >
              <a href="/usuarios/eliminar/<%=user.id%>" type="submit" class="btn btn-danger mb-3"
                >Eliminar</a
              >
              <% } %>
            </td>
          </tr>
        </tbody>
      </table>
    </div>

